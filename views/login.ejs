<%- include("partials/header") -%>
<div class="login-page">
<h1>Login</h1>
<p><%=logincontent%></p>
<form action="/compose" method="post">
    <div class="form-group">
        <label>Username</label>
        <input type="text" class="form-control" name="username" id="" autocomplete="off">
    </div>
    <div class="form-group">
        <label>Password</label>
        <input type="password" class="form-control" name="password" id="" autocomplete="off">
    </div>
    <button type="submit" name="login" value ="loginpage" class="btn btn-primary">Sign in</button>

</form>
</div>

<%- include("partials/footer") -%>